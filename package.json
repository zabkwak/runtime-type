{
  "name": "runtime-type",
  "version": "1.2.0",
  "description": "Module for casting to types at runtime.",
  "main": "index.js",
  "types": "./dist/__types__/index.d.ts",
  "scripts": {
    "test": "mocha __tests__ --recursive --compilers js:babel-core/register",
    "create_index": "run-script-os",
    "create_index:win32": "echo module.exports = require('./dist').default; > index.js",
    "create_index:linux:darwin": "echo \"module.exports = require('./dist').default;\" > index.js",
    "build_es6": "./node_modules/.bin/babel ./src --out-dir ./dist --copy-files && npm run create_index",
    "clear": "rm -rf ./dist && rm -f ./index.js",
    "prepublish": "npm run build_es6",
    "postpublish": "npm run clear"
  },
  "author": "Lukáš Mačuda <lukas.macuda@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "smart-error": "^1.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "run-script-os-fix": "^1.0.4"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zabkwak/runtime-type.git"
  },
  "bugs": {
    "url": "https://github.com/zabkwak/runtime-type/issues"
  },
  "homepage": "https://github.com/zabkwak/runtime-type#readme",
  "keywords": [
    "data-type",
    "runtime",
    "casting"
  ],
  "readme": "README.md"
}
